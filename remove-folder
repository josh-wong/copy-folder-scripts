# Python program to demonstrate shutil.rmtree()
 
import shutil
import os
 
# Location: the parent folder where the files to be removed originate from
location = "<INSERT FULL FOLDER PATH>"

# Directory: the folder that should be deleted
dir = "<INSERT FULL FOLDER PATH>"
 
# path
path = os.path.join(location, dir)
 
# removing directory
shutil.rmtree(path)
